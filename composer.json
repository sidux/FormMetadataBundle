{
    "name":"malwarebytes/form-metadata-bundle",
    "type": "symfony-bundle",
    "description":"Use annotations with the forms builder e.g. @Form\\Field(\"text\"). Associates metadata along with the entity class. Useful to make more generic controllers that can deal with dynamic entities (such as CMS scenarios).",
    "keywords": ["form","annotations","metadata"],
    "homepage":"https://github.com/Malwarebytes/FormMetadataBundle",
    "license": "MIT",
    "authors": [
        {
        "name": "Jonathan Chan",
	    "email": "jchan@malwarebytes.org",
	    "role": "Developer"
	},
        {
        "name": "Paul Hendryx",
	    "email": "phendryx@malwarebytes.org",
	    "role": "Developer"
	},
        {
        "name": "Cameron Manderson",
	    "email": "cameronmanderson@gmail.com",
	    "role": "Original Author"
	}
    ],
    "require": {
        "php": ">=5.3.3"
    },
    "require-dev": {
        "symfony/symfony": "~2.3",
        "doctrine/annotations":"1.1.2"
    },
    "autoload": {
        "psr-0": {
            "Malwarebytes\\FormMetadataBundle": ""
        }
    },
    "target-dir":"Malwarebytes/FormMetadataBundle"
}
